{
  "name": "bowhead-js",
  "version": "1.1.1",
  "description": "Tiny string interpolation library",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "author": "Mgen (https://www.mgenware.com)",
  "homepage": "https://github.com/mgenware/bowhead-js",
  "bugs": "https://github.com/mgenware/bowhead-js/issues",
  "keywords": [
    "layit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mgenware/bowhead-js.git"
  },
  "license": "MIT",
  "scripts": {
    "test": "yarn run clean && yarn run compile && yarn run lint && yarn run t",
    "t": "mocha dist_tests/*.js",
    "build": "cross-env NODE_ENV=production yarn test",
    "compile": "tsc -b tests",
    "dev": "cross-env NODE_ENV=development yarn run clean && tsc -b tests -w",
    "prepublishOnly": "yarn run test",
    "lint": "eslint --max-warnings 0 --ext .ts src/ tests/",
    "clean": "del-cli dist && del-cli dist_tests"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/dist/**/*.js",
    "/dist/**/*.ts",
    "/dist/**/*.map"
  ],
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.7",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "cross-env": "^6.0.3",
    "del-cli": "^3.0.0",
    "eslint": "^6.6.0",
    "mocha": "^6.2.2",
    "typescript": "^3.7.2"
  },
  "dependencies": {}
}
